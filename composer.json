{
  "name": "zanphp/ws-server",
  "require": {
    "php": ">=5.6",
    "zanphp/server-base": "dev-master",
    "zanphp/routing": "dev-master",
    "zanphp/coroutine": "dev-master",
    "zanphp/container": "dev-master",
    "zanphp/exception": "dev-master",
    "zanphp/timer": "dev-master",
    "zanphp/support": "dev-master",
    "zanphp/http-server": "dev-master",
    "zanphp/tcp-server": "dev-master",
    "zanphp/contracts": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\WSServer\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}