{
  "name": "zanphp/ws-server",
  "repositories": {
    "0": {
      "type": "git",
      "url": "https://github.com/sukui/server-base.git"
    }
  },
  "require": {
    "php": ">=5.6",
    "zanphp/server-base": "dev-laogui"
  },
  "autoload": {
    "psr-4": {
      "Zan\\Framework\\Network\\WebSocket\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}